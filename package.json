    "@mui/material": "^5.5.3",
    "@testing-library/jest-dom": "^5.16.2",
    "date-fns": "^2.28.0",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.2",
    "react-scripts": "5.0.0",
    "react-toastify": "^8.2.0",
    "recoil": "^0.7.2",
    "recoil-persist": "^4.1.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
