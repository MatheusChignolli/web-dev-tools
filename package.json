    "eject": "craco eject",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\""
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.8.2",
